<%= form_with local: true do |f| %>
  <%= f.label :flickr_id %>
  <%= f.text_field :flickr_id %>
  <%= f.submit %>
<% end %>

<% unless @photos.nil? %>
  <% @photos.each do |photo| %>
    <%= image_tag("https://farm#{photo.farm}.staticflickr.com/#{photo.server}/#{photo.id}_#{photo.secret}.jpg") %>
  <% end %>
<% end %>